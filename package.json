{
  "name": "fractals",
  "version": "1.0.0",
  "description": "Fractal Explorer",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx src",
    "client:dev": "webpack-dev-server --config config/webpack.dev.js",
    "test:unit": "jest",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "check:conflicts": "node ./check-conflicts.js",
    "prettier:check": "prettier --check '{src,test}/**/*.{js,jsx,css,less}'",
    "prettier:fix": "prettier --write '{src,test}/**/*.{js,jsx,css,less}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "npm run check:conflicts",
    "{src,test}/**/*.{js,jsx,css,less}": [
      "prettier --write"
    ]
  },
  "author": "Mark Timothy Macdonald",
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/node": "^7.8.7",
